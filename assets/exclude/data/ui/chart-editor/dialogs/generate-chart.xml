<?xml version="1.0" encoding="utf-8"?>
<dialog id="chartDialog" width="600" height="400" title="Generate Chart">
	<vbox width="100%" height="100%">
		<vbox id="midiEntryContainer" width="100%" />
		<vbox width="100%" height="100%" id="generateDataContainer" horizontalAlign="center">
			<frame text="Channels" width="100%" height="100%">
				<scrollview id="channelView" contentWidth="100%" width="100%" height="100%"/>
			</frame>
		</vbox>
		<vbox width="100%" height="100%" horizontalAlign="center">
			<hbox width="100%">
				<label text="Algorithm" verticalAlign="center"/>
				<dropdown id="algorithmDropdown" value="" width="200" height="25" dropdownSize="10" dropdownWidth="200" searchable="true" verticalAlign="center"/>
			</hbox>
			<frame text="Algorithm Params" width="100%" height="100%">
				<scrollview id="paramView" contentWidth="100%" width="100%" height="100%"/>
			</frame>
		</vbox>
		<hbox width="100%" horizontalAlign="right">
			<button text="Cancel" id="dialogCancel" />
			<spacer width="100%" />
			<button text="Generate Hints" id="dialogHints" disabled="true" />
			<button text="Generate Notes" id="dialogNotes" disabled="true"/>
		</hbox>
	</vbox>
</dialog>
